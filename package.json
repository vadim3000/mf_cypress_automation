    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "cypress": "^10.3.1",
    "cypress-rollup-preprocessor": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.77.1",
    "start-server-and-test": "^1.14.0"
  }
